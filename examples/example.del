class Range {
    start: int;
    end: int;
}

function sum(r : Range) : int {
    let sum = 0;
    for (let i = r.start; i <= r.end; i = i + 1) {
        sum = sum + i;
        print(sum);
    }
    return sum;
}

function main() {
    let range = new Range(0, 10);
    let sum = sum(range);
    print(sum);
    print(range);
}
